{
    "rewrites": [{ "source": "/(.*)", "destination": "/api/$1" }],
    "headers": [{
        "source": "/(.*)",
        "headers": [{ "key": "access-control-allow-origin", "value": "*" }]
    }],
    "redirects": [{ "source": "/", "destination": "https://github.com/benborgers/driver" }]
    "github": { "silent": true }
}
